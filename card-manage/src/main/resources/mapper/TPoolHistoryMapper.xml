<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smart.card.manage.mapper.TPoolHistoryMapper">


    <select id="tPoolHistoryList" parameterType="com.smart.card.manage.entity.TPoolHistory" resultType="com.smart.card.manage.entity.TPoolHistory">
        SELECT * from t_pool t where t.delete_flag = 0
        <if test="vo.id != null ">
            and t.id = #{vo.id}
        </if>
        <if test="vo.type != null ">
            and t.type = #{vo.type}
        </if>
        <if test="vo.mainStatus != null ">
            and t.main_status = #{vo.mainStatus}
        </if>
        <if test="vo.deptIds != null and vo.deptIds != '' ">
            and t.dept_id in (${vo.deptIds})
        </if>
    </select>


</mapper>
