<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smart.card.manage.mapper.TPoolPackageMemberUseMapper">


    <select id="tPoolPackageMemberUseList" parameterType="com.smart.card.manage.entity.TPoolPackageMemberUse" resultType="com.smart.card.manage.entity.TPoolPackageMemberUse">
        SELECT * from t_pool_package_member_use t where t.delete_flag = 0
        <if test="vo.poolNbr != null ">
            and t.pool_nbr = #{vo.poolNbr}
        </if>
        <if test="vo.date != null and vo.date != '' ">
            and t.date = #{vo.date}
        </if>
        <if test="vo.subsId != null and vo.subsId != '' ">
            and t.subs_id = #{vo.subsId}
        </if>
        <if test="vo.deptIds != null and vo.deptIds != '' ">
            and t.dept_id in (${vo.deptIds})
        </if>
    </select>


</mapper>
