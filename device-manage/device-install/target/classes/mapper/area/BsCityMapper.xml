<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smart.device.install.mapper.area.BsCityMapper">

    <select id="queryCitysByCode" resultType="com.smart.device.common.install.entity.TBaseRegion" parameterType="java.lang.String">
        select t.city_code regionCode,t.city_name regionName,t.level from bs_city t
         where t.province_code = #{code}
    </select>


</mapper>
