<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smart.device.install.mapper.inspection.THisInspectionDetailsMapper">

	<select id="getHisInsDetailsList" resultType="com.smart.device.common.install.entity.THisInspectionDetails"
			parameterType="com.smart.device.common.install.entity.THisInspectionDetails" >
 		SELECT * FROM t_his_inspection_details t WHERE t.delete_flag = 0
		<if test="vo.inspectionId != null ">
			and t.inspection_id = #{vo.inspectionId}
		</if>
		<if test="vo.fireEngineAccess != null ">
			and t.fire_engine_access = #{vo.fireEngineAccess}
		</if>
		<if test="vo.deviceStatus != null ">
			and t.device_status = #{vo.deviceStatus}
		</if>
 	</select>

	<select id="selectId" resultType= "com.smart.device.common.install.entity.THisInspectionDetails"
			parameterType="java.lang.Long">
		SELECT * from  t_his_inspection_details where 1=1
		<if test="fireId != null ">
			and fire_id = #{fireId}
		</if>
	</select>



</mapper>
