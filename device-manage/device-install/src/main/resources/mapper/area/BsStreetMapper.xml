<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smart.device.install.mapper.area.BsStreetMapper">

    <select id="queryStreetsByCode" resultType="com.smart.device.common.install.entity.TBaseRegion" parameterType="java.lang.String">
       select t.street_code regionCode,t.street_name regionName,t.level from bs_street t
       where t.county_code = #{code}
    </select>

</mapper>
