<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smart.device.install.mapper.area.BsVillageMapper">

    <select id="queryVillagesByCode" resultType="com.smart.device.common.install.entity.TBaseRegion" parameterType="java.lang.String">
        select t.village_code regionCode,t.village_name regionName,t.level from bs_village t
        where t.street_code = #{code}

    </select>

</mapper>
