<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smart.device.access.mapper.TDeviceProtocolMapper">

    <select id="queryProtocolList" resultType="com.smart.device.common.access.entity.TDeviceProtocol" >
        select * from t_device_protocol where delete_flag = 0
    </select>






    <select id="getTypeByName" resultType="String">
        select protocol_type  protocolType from t_device_protocol
        where protocol_name = #{protocol}
    </select>


</mapper>
