<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smart.brd.manage.base.mapper.TDeviceExtMapper">


    <select id="tDeviceExtList" parameterType="com.smart.brd.manage.base.entity.TDeviceExt" resultType="com.smart.brd.manage.base.entity.TDeviceExt">
        SELECT * from t_device_ext t where t.delete_flag = 0
        <if test="vo.deptIds != null and vo.deptIds != '' ">
            and t.dept_id in (${vo.deptIds})
        </if>
        ORDER BY t.create_time desc
    </select>


</mapper>
