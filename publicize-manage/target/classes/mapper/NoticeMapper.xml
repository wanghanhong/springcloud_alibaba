<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smart.publicize.manage.mapper.NoticeMapper">

    <select id="queryNoticeListLimit" resultType="com.smart.publicize.manage.entity.NoticeEntity">
        select * from t_publicize_notice t
        order by t.create_time desc limit 10
    </select>


</mapper>
